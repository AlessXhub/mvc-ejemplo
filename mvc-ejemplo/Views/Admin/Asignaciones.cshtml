@{
}
@model List<SistemaWebEscolar.Models.ProfesorMaterialModelo>

<h2>Asignaciones de Profesores</h2>

<table border="1">
    <thead>
        <tr>
            <th>Profesor</th>
            <th>Grado</th>
            <th>Materia</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.NombreProfesor</td>
                <td>@item.NombreGrado</td>
                <td>@item.NombreMateria</td>
            </tr>
        }
    </tbody>
</table>